config database
	option name 'IPv4_Blacklist'
	option family 'ipv4'
	option enabled '1'
	option path '/etc/fence/ipv4.mmdb'
	option ratio '1.0'
	option queue '10'

config database
	option name 'IPv6_Blacklist'
	option family 'ipv6'
	option enabled '1'
	option path '/etc/fence/ipv6.mmdb'
	option ratio '1.0'
	option queue '20'
